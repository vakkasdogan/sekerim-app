<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/egitim_katolog_page"
    android:background="@drawable/egitim_arka"
    tools:ignore="MissingDefaultResource">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <!-- kan sekeri -->
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/kansekeri"
                android:padding="10dp"
                android:layout_marginTop="50dp">

                <ImageView
                    android:layout_width="92dp"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/kansekeri1" />

                <ImageView
                    android:layout_width="378dp"
                    android:layout_height="98dp"
                    android:onClick="gotokansekeriolcum"
                    app:srcCompat="@drawable/kansekeributon" />
            </LinearLayout>

            <!-- insulin uyg -->
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/insulinuyg"
                android:padding="10dp"
                android:layout_below="@id/kansekeri">

                <ImageView
                    android:layout_width="89dp"
                    android:layout_height="92dp"
                    android:layout_gravity="center_vertical"
                    app:srcCompat="@drawable/insulin1" />

                <ImageView
                    android:layout_width="369dp"
                    android:layout_height="73dp"
                    android:layout_gravity="center_vertical"
                    android:onClick="gotoinsulinguyg"
                    app:srcCompat="@drawable/insulinbutton" />
            </LinearLayout>

            <!-- beslenme -->
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/beswlenme"
                android:padding="10dp"
                android:layout_below="@id/insulinuyg">

                <ImageView
                    android:layout_width="87dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    app:srcCompat="@drawable/beslenme1" />

                <ImageView
                    android:layout_width="365dp"
                    android:layout_height="91dp"
                    android:layout_gravity="center_vertical"
                    android:onClick="gotobeslenme"
                    app:srcCompat="@drawable/beslenmebutton" />
            </LinearLayout>

            <!-- oralilaclar -->
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/oralilaclar"
                android:padding="10dp"
                android:layout_below="@id/beswlenme">

                <ImageView
                    android:layout_width="89dp"
                    android:layout_height="85dp"
                    android:layout_gravity="center_vertical"
                    app:srcCompat="@drawable/ilaclar1" />

                <ImageView
                    android:layout_width="369dp"
                    android:layout_height="99dp"
                    android:layout_gravity="center_vertical"
                    android:onClick="gotoilaclar"
                    app:srcCompat="@drawable/oralantidiyabetikilaclarbuton" />
            </LinearLayout>

            <!-- egzersiz -->
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/egzersiz"
                android:padding="10dp"
                android:layout_below="@id/oralilaclar">

                <ImageView
                    android:layout_width="91dp"
                    android:layout_height="104dp"
                    android:layout_gravity="center_vertical"
                    app:srcCompat="@drawable/egzersiz1" />

                <ImageView
                    android:layout_width="366dp"
                    android:layout_height="77dp"
                    android:layout_gravity="center_vertical"
                    android:onClick="gotoegzersiz"
                    app:srcCompat="@drawable/egzersizbuton" />
            </LinearLayout>

            <!-- hipoglisemi -->
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/hipoglisemi"
                android:padding="10dp"
                android:layout_below="@id/egzersiz">

                <ImageView
                    android:layout_width="91dp"
                    android:layout_height="93dp"
                    android:layout_gravity="center_vertical"
                    android:onClick="gotohipoglisemi"
                    app:srcCompat="@drawable/hipoglisemi1" />

                <ImageView
                    android:layout_width="366dp"
                    android:layout_height="74dp"
                    android:layout_gravity="center_vertical"
                    android:onClick="gotohipoglisemi"
                    app:srcCompat="@drawable/hipoglisemibuton" />
            </LinearLayout>

            <!-- hiperglisemi -->
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/hiperglisemi"
                android:padding="10dp"
                android:layout_below="@id/hipoglisemi">

                <ImageView
                    android:layout_width="96dp"
                    android:layout_height="104dp"
                    android:layout_gravity="center_vertical"
                    android:onClick="gotohiperglisemi"
                    app:srcCompat="@drawable/hiperglisemi1" />

                <ImageView
                    android:layout_width="368dp"
                    android:layout_height="87dp"
                    android:onClick="gotohiperglisemi"
                    android:layout_gravity="center_vertical"
                    app:srcCompat="@drawable/hipergilsemibuton" />
            </LinearLayout>

            <!-- beslenme -->
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/ayakbakimi"
                android:padding="10dp"
                android:layout_below="@id/hiperglisemi">

                <ImageView
                    android:layout_width="92dp"
                    android:layout_height="83dp"
                    android:layout_gravity="center_vertical"
                    app:srcCompat="@drawable/ayakbakimi1" />

                <ImageView
                    android:layout_width="375dp"
                    android:layout_height="78dp"
                    android:layout_gravity="center_vertical"
                    app:srcCompat="@drawable/ayakbakimibuton" />
            </LinearLayout>

        </RelativeLayout>
    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>